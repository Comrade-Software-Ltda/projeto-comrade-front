{
  "name": "comrade-project",
  "version": "0.0.1",
  "scripts": {
    "start:debug": "node .bless.js && node ./node_modules/@angular/cli/bin/ng serve --configuration=debug --hmr",
    "start:optimized": "node .bless.js && node ./node_modules/@angular/cli/bin/ng serve --configuration=optimized --hmr",
    "build:analyze": "ng build --stats-json",
    "build:prd": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration=production",
    "lite-server": "lite-server --baseDir='dist'",
    "watch": "ng build --watch --configuration=debug",
    "express": "node express/app.js",
    "test": "ng test",
    "lint": "eslint --color --fix -c .eslintrc.json --ext .ts ./src/app",
    "format:override": "prettier --write --ignore-path .gitignore \"**/*.{js,ts,json,html}\"",
    "format:enforce": "prettier --check --ignore-path .gitignore \"**/*.{js,ts,json,html}\"",
    "stubs": "stubby -s 1500 -d stubbs/stubby.json -w",
    "stubby": "concurrently \"npm run start:debug\" \"npm run stubs\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~14.2.1",
    "@angular/cdk": "^14.2.1",
    "@angular/common": "^14.2.1",
    "@angular/core": "~14.2.1",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.2.1",
    "@angular/platform-browser": "^14.2.1",
    "@angular/platform-browser-dynamic": "^14.2.1",
    "@angular/router": "~14.2.1",
    "@popperjs/core": "^2.11.6",
    "concurrently": "^7.4.0",
    "core-js": "^3.25.1",
    "destroy": "^1.2.0",
    "devextreme": "^22.1.5",
    "devextreme-angular": "^22.1.5",
    "devextreme-schematics": "^1.4.2",
    "jasmine": "^4.4.0",
    "jquery": "^3.6.1",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.4",
    "ngx-currency": "^2.5.3",
    "ngx-mask": "^14.2.1",
    "popper.js": "^1.16.1",
    "rxjs": "~7.5.6",
    "stubby": "^5.1.0",
    "tslib": "^2.4.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1402.2",
    "@angular-devkit/build-angular": "^14.2.2",
    "@angular-eslint/builder": "^14.0.4",
    "@angular-eslint/eslint-plugin": "^14.0.4",
    "@angular-eslint/eslint-plugin-template": "^14.0.4",
    "@angular-eslint/schematics": "^14.0.4",
    "@angular-eslint/template-parser": "^14.0.4",
    "@angular/cli": "~14.2.2",
    "@angular/compiler": "^14.2.1",
    "@angular/compiler-cli": "^14.2.1",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.7.16",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.36.2",
    "@typescript-eslint/parser": "5.36.2",
    "devextreme-cldr-data": "^1.0.3",
    "devextreme-cli": "latest",
    "devextreme-themebuilder": "^22.1.5",
    "eslint": "^8.23.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "globalize": "^1.7.0",
    "jasmine-core": "~4.4.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.0.0",
    "lite-server": "^2.6.1",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "protractor": "^7.0.0",
    "ts-node": "~10.9.1",
    "typescript": "~4.8.3"
  },
  "resolutions": {
    "webpack": "5.64.4"
  }
}
