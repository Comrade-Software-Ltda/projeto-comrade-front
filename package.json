{
  "name": "comrade-project",
  "version": "0.0.1",
  "scripts": {
    "start:debug": "node .bless.js && node ./node_modules/@angular/cli/bin/ng serve --configuration=debug --hmr",
    "start:optimized": "node .bless.js && node ./node_modules/@angular/cli/bin/ng serve --configuration=optimized --hmr",
    "build:analyze": "ng build --stats-json",
    "build:prd": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --configuration=production",
    "lite-server": "lite-server --baseDir='dist'",
    "watch": "ng build --watch --configuration=debug",
    "express": "node express/app.js",
    "test": "ng test",
    "lint": "eslint --color --fix -c .eslintrc.json --ext .ts ./src/app",
    "format:override": "prettier --write --ignore-path .gitignore \"**/*.{js,ts,json,html}\"",
    "format:enforce": "prettier --check --ignore-path .gitignore \"**/*.{js,ts,json,html}\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~17.0.8",
    "@angular/cdk": "^17.0.4",
    "@angular/common": "^17.0.8",
    "@angular/core": "~17.0.8",
    "@angular/flex-layout": "^15.0.0-beta.42",
    "@angular/forms": "^17.0.8",
    "@angular/platform-browser": "^17.0.8",
    "@angular/platform-browser-dynamic": "^17.0.8",
    "@angular/router": "~17.0.8",
    "@popperjs/core": "^2.11.8",
    "core-js": "^3.35.0",
    "destroy": "^1.2.0",
    "devextreme": "^23.2.3",
    "devextreme-angular": "^23.2.3",
    "devextreme-schematics": "^1.6.5",
    "jasmine": "^5.1.0",
    "jwt-decode": "^4.0.0",
    "moment": "^2.30.1",
    "ngx-mask": "^17.0.4",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "uuid": "^9.0.1",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.1700.9",
    "@angular-devkit/build-angular": "^17.0.9",
    "@angular-eslint/builder": "^17.2.0",
    "@angular-eslint/eslint-plugin": "^17.2.0",
    "@angular-eslint/eslint-plugin-template": "^17.2.0",
    "@angular-eslint/schematics": "^17.2.0",
    "@angular-eslint/template-parser": "^17.2.0",
    "@angular/cli": "~17.0.9",
    "@angular/compiler": "^17.0.8",
