    "build:prd": "node --max_old_space_size=4096 ./node_modules/@angular/cli/bin/ng build --configurations=production",
    "lite-server": "lite-server --baseDir='dist'",
    "express": "node express/app.js",
    "test": "ng test",
    "lint": "eslint --color --fix -c .eslintrc.json --ext .ts ./src/app",
    "format": "prettier --write --ignore-path .gitignore '**/*.{js,ts,json,html}'",
    "format:enforce": "prettier --check --ignore-path .gitignore '**/*.{js,ts,json,html}'"
  },
  "private": true,
